{
  "name": "quirofisicos-rocha",
  "version": "1.0.0",
  "description": "Quirofísicos Rocha appointment management system",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "sequelize": "sequelize",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:seed": "sequelize db:seed:all",
    "db:reset": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "db:setup": "node scripts/setup-db.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "echo \"No linter configured\"",
    "build": "echo \"No build step required for this project\""
  },
  "dependencies": {
    "@vonage/server-sdk": "^3.22.1",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "appointment",
